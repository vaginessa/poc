<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hawk.ecom.svp.persist.mapper.BsiOrderDetailMapper">

	<sql id="columns">	
				id AS "id" ,
		svp_order_id AS "svpOrderId" ,
		bsi_phone_model_id AS "bsiPhoneModelId" ,
		bsi_product_id AS "bsiProductId" ,
		imei AS "imei" ,
		bsi_id_typ AS "bsiIdTyp" ,
		bsi_id_number AS "bsiIdNumber" ,
		bsi_birthday AS "bsiBirthday" ,
		bsi_sex AS "bsiSex" ,
		bsi_name AS "bsiName" 
	</sql>
	
	<sql id="where">
		<trim prefix="WHERE" prefixOverrides="AND|OR">		 
			<if test="id != null"> AND id = #{id}  </if>
			<if test="svpOrderId != null"> AND svp_order_id = #{svpOrderId}  </if>
			<if test="bsiPhoneModelId != null"> AND bsi_phone_model_id = #{bsiPhoneModelId}  </if>
			<if test="bsiProductId != null"> AND bsi_product_id = #{bsiProductId}  </if>
			<if test="imei != null"> AND imei = #{imei}  </if>
			<if test="bsiIdTyp != null"> AND bsi_id_typ = #{bsiIdTyp}  </if>
			<if test="bsiIdNumber != null"> AND bsi_id_number = #{bsiIdNumber}  </if>
			<if test="bsiBirthday != null"> AND bsi_birthday = #{bsiBirthday}  </if>
			<if test="bsiSex != null"> AND bsi_sex = #{bsiSex}  </if>
			<if test="bsiName != null"> AND bsi_name = #{bsiName}  </if>
		</trim>	
	</sql>
	
	<sql id="where_old">
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			AND id = #{old_id}
			AND svp_order_id = #{old_svpOrderId}
			AND bsi_phone_model_id = #{old_bsiPhoneModelId}
			AND bsi_product_id = #{old_bsiProductId}
			AND imei = #{old_imei}
			AND bsi_id_typ = #{old_bsiIdTyp}
			AND bsi_id_number = #{old_bsiIdNumber}
			AND bsi_birthday = #{old_bsiBirthday}
			AND bsi_sex = #{old_bsiSex}
			AND bsi_name = #{old_bsiName}
		</trim>
	</sql>
	
	<sql id="update">
		<set>
			id = #{id},
			svp_order_id = #{svpOrderId},
			bsi_phone_model_id = #{bsiPhoneModelId},
			bsi_product_id = #{bsiProductId},
			imei = #{imei},
			bsi_id_typ = #{bsiIdTyp},
			bsi_id_number = #{bsiIdNumber},
			bsi_birthday = #{bsiBirthday},
			bsi_sex = #{bsiSex},
			bsi_name = #{bsiName},
		</set>
	</sql>
	
	<sql id="updateWithoutNull">
		<set>
			<if test="id != null">id = #{id},</if>
			<if test="svpOrderId != null">svp_order_id = #{svpOrderId},</if>
			<if test="bsiPhoneModelId != null">bsi_phone_model_id = #{bsiPhoneModelId},</if>
			<if test="bsiProductId != null">bsi_product_id = #{bsiProductId},</if>
			<if test="imei != null">imei = #{imei},</if>
			<if test="bsiIdTyp != null">bsi_id_typ = #{bsiIdTyp},</if>
			<if test="bsiIdNumber != null">bsi_id_number = #{bsiIdNumber},</if>
			<if test="bsiBirthday != null">bsi_birthday = #{bsiBirthday},</if>
			<if test="bsiSex != null">bsi_sex = #{bsiSex},</if>
			<if test="bsiName != null">bsi_name = #{bsiName},</if>
		</set>
	</sql>
	
	<sql id="pkwhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			AND id = #{id}
		</trim>
	</sql>
	
	<select id="load"  resultType="BsiOrderDetailDomain">
		SELECT 
		<include refid="columns" />
		FROM t_svp_bsi_order_detail
		<include refid="pkwhere" />		
	</select>
	
	<select id="loadDynamic" parameterType="hashmap" resultType="BsiOrderDetailDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_svp_bsi_order_detail
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
	</select>

	<select id="count"  parameterType="hashmap" resultType="integer">
		SELECT count(1) 
		FROM t_svp_bsi_order_detail
		<include refid="where" />
	</select>
	
	<select id="countByPK"  resultType="integer">
		SELECT count(1) 
		FROM t_svp_bsi_order_detail
		<include refid="pkwhere" />		
	</select>
	
	<insert id="insert"  parameterType="BsiOrderDetailDomain">
		INSERT INTO t_svp_bsi_order_detail (			
				id,				
				svp_order_id,				
				bsi_phone_model_id,				
				bsi_product_id,				
				imei,				
				bsi_id_typ,				
				bsi_id_number,				
				bsi_birthday,				
				bsi_sex,				
				bsi_name				
		)		
		VALUES (	
			#{id},
			#{svpOrderId},
			#{bsiPhoneModelId},
			#{bsiProductId},
			#{imei},
			#{bsiIdTyp},
			#{bsiIdNumber},
			#{bsiBirthday},
			#{bsiSex},
			#{bsiName}
		) 
	</insert>
	
	<delete id="delete" >
		DELETE FROM t_svp_bsi_order_detail
		<include refid="pkwhere" />	
	</delete>
	
	<delete id="deleteDynamic" parameterType="hashmap">
		DELETE FROM t_svp_bsi_order_detail
		<include refid="where" />
	</delete>

	<update id="update" parameterType="BsiOrderDetailDomain">
		UPDATE t_svp_bsi_order_detail	
		<include refid="update" />
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateWithoutNull" parameterType="BsiOrderDetailDomain">
		UPDATE t_svp_bsi_order_detail
		<include refid="updateWithoutNull"/>
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateDynamic" parameterType="hashmap">
		UPDATE t_svp_bsi_order_detail	
		<include refid="update" />
		<include refid="where_old" />
	</update>
	
	<select id="loadDynamicPaging" parameterType="hashmap" resultType="BsiOrderDetailDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_svp_bsi_order_detail
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
		<if test="_offset != null">LIMIT #{_offset},#{_limit}</if>	
	</select>
</mapper>