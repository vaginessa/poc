<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hawk.framework.dic.mapper.DataDefinitionMapper">

	<sql id="columns">
				object_id AS "objectId" ,
		use_type AS "useType" ,
		data_type AS "dataType" ,
		object_label AS "objectLabel" ,
		description AS "description" ,
		cname AS "cname" ,
		regex AS "regex" ,
		max_length AS "maxLength" ,
		min_length AS "minLength" ,
		max_value AS "maxValue" ,
		min_value AS "minValue" 
	</sql>
	
	<sql id="where">
		<trim prefix="WHERE" prefixOverrides="AND|OR">		 
			<if test="objectId != null"> AND object_id = #{objectId}  </if>
			<if test="useType != null"> AND use_type = #{useType}  </if>
			<if test="dataType != null"> AND data_type = #{dataType}  </if>
			<if test="objectLabel != null"> AND object_label = #{objectLabel}  </if>
			<if test="description != null"> AND description = #{description}  </if>
			<if test="cname != null"> AND cname = #{cname}  </if>
			<if test="regex != null"> AND regex = #{regex}  </if>
			<if test="maxLength != null"> AND max_length = #{maxLength}  </if>
			<if test="minLength != null"> AND min_length = #{minLength}  </if>
			<if test="maxValue != null"> AND max_value = #{maxValue}  </if>
			<if test="minValue != null"> AND min_value = #{minValue}  </if>
		</trim>	
	</sql>
	
	<sql id="where_old">
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			AND object_id = #{old_objectId}
			AND use_type = #{old_useType}
			AND data_type = #{old_dataType}
			AND object_label = #{old_objectLabel}
			AND description = #{old_description}
			AND cname = #{old_cname}
			AND regex = #{old_regex}
			AND max_length = #{old_maxLength}
			AND min_length = #{old_minLength}
			AND max_value = #{old_maxValue}
			AND min_value = #{old_minValue}
		</trim>
	</sql>
	
	<sql id="update">
		<set>
			object_id = #{objectId},
			use_type = #{useType},
			data_type = #{dataType},
			object_label = #{objectLabel},
			description = #{description},
			cname = #{cname},
			regex = #{regex},
			max_length = #{maxLength},
			min_length = #{minLength},
			max_value = #{maxValue},
			min_value = #{minValue},
		</set>
	</sql>
	
	<sql id="updateWithoutNull">
		<set>
			<if test="objectId != null">object_id = #{objectId},</if>
			<if test="useType != null">use_type = #{useType},</if>
			<if test="dataType != null">data_type = #{dataType},</if>
			<if test="objectLabel != null">object_label = #{objectLabel},</if>
			<if test="description != null">description = #{description},</if>
			<if test="cname != null">cname = #{cname},</if>
			<if test="regex != null">regex = #{regex},</if>
			<if test="maxLength != null">max_length = #{maxLength},</if>
			<if test="minLength != null">min_length = #{minLength},</if>
			<if test="maxValue != null">max_value = #{maxValue},</if>
			<if test="minValue != null">min_value = #{minValue},</if>
		</set>
	</sql>
	
	<sql id="pkwhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			AND object_id = #{objectId}
		</trim>
	</sql>
	
	<select id="load"  resultType="DataDefinitionDomain">
		SELECT 
		<include refid="columns" />
		FROM t_dic_data_definition
		<include refid="pkwhere" />		
	</select>
	
	<select id="loadDynamic" parameterType="hashmap" resultType="DataDefinitionDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_data_definition
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
	</select>

	<select id="count"  parameterType="hashmap" resultType="integer">
		SELECT count(1) 
		FROM t_dic_data_definition
		<include refid="where" />
	</select>
	
	<select id="countByPK"  resultType="integer">
		SELECT count(1) 
		FROM t_dic_data_definition
		<include refid="pkwhere" />		
	</select>
	
	<insert id="insert"  parameterType="DataDefinitionDomain">
		INSERT INTO t_dic_data_definition (			
				object_id,				
				use_type,				
				data_type,				
				object_label,				
				description,				
				cname,				
				regex,				
				max_length,				
				min_length,				
				max_value,				
				min_value				
		)		
		VALUES (	
			#{objectId},
			#{useType},
			#{dataType},
			#{objectLabel},
			#{description},
			#{cname},
			#{regex},
			#{maxLength},
			#{minLength},
			#{maxValue},
			#{minValue}
		) 
	</insert>
	
	<delete id="delete" >
		DELETE FROM t_dic_data_definition
		<include refid="pkwhere" />	
	</delete>
	
	<delete id="deleteDynamic" parameterType="hashmap">
		DELETE FROM t_dic_data_definition
		<include refid="where" />
	</delete>

	<update id="update" parameterType="DataDefinitionDomain">
		UPDATE t_dic_data_definition	
		<include refid="update" />
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateWithoutNull" parameterType="DataDefinitionDomain">
		UPDATE t_dic_data_definition
		<include refid="updateWithoutNull"/>
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateDynamic" parameterType="hashmap">
		UPDATE t_dic_data_definition	
		<include refid="update" />
		<include refid="where_old" />
	</update>
	
	<select id="loadDynamicPaging" parameterType="hashmap" resultType="DataDefinitionDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_data_definition
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
		<if test="_offset != null">LIMIT #{_offset},#{_limit}</if>	
	</select>
</mapper>