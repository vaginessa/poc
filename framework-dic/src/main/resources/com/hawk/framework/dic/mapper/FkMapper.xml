<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hawk.framework.dic.mapper.FkMapper">

	<sql id="columns">	
				object_id AS "objectId" ,
		object_code AS "objectCode" ,
		object_name AS "objectName" ,
		object_comment AS "objectComment" ,
		parent_table_object_id AS "parentTableObjectId" ,
		child_table_object_id AS "childTableObjectId" 
	</sql>
	
	<sql id="where">
		<trim prefix="WHERE" prefixOverrides="AND|OR">		 
			<if test="objectId != null"> AND object_id = #{objectId}  </if>
			<if test="objectCode != null"> AND object_code = #{objectCode}  </if>
			<if test="objectName != null"> AND object_name = #{objectName}  </if>
			<if test="objectComment != null"> AND object_comment = #{objectComment}  </if>
			<if test="parentTableObjectId != null"> AND parent_table_object_id = #{parentTableObjectId}  </if>
			<if test="childTableObjectId != null"> AND child_table_object_id = #{childTableObjectId}  </if>
		</trim>	
	</sql>
	
	<sql id="where_old">
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			AND object_id = #{old_objectId}
			AND object_code = #{old_objectCode}
			AND object_name = #{old_objectName}
			AND object_comment = #{old_objectComment}
			AND parent_table_object_id = #{old_parentTableObjectId}
			AND child_table_object_id = #{old_childTableObjectId}
		</trim>
	</sql>
	
	<sql id="update">
		<set>
			object_id = #{objectId},
			object_code = #{objectCode},
			object_name = #{objectName},
			object_comment = #{objectComment},
			parent_table_object_id = #{parentTableObjectId},
			child_table_object_id = #{childTableObjectId},
		</set>
	</sql>
	
	<sql id="updateWithoutNull">
		<set>
			<if test="objectId != null">object_id = #{objectId},</if>
			<if test="objectCode != null">object_code = #{objectCode},</if>
			<if test="objectName != null">object_name = #{objectName},</if>
			<if test="objectComment != null">object_comment = #{objectComment},</if>
			<if test="parentTableObjectId != null">parent_table_object_id = #{parentTableObjectId},</if>
			<if test="childTableObjectId != null">child_table_object_id = #{childTableObjectId},</if>
		</set>
	</sql>
	
	<sql id="pkwhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			AND object_id = #{objectId}
		</trim>
	</sql>
	
	<select id="load"  resultType="FkDomain">
		SELECT 
		<include refid="columns" />
		FROM t_dic_fk
		<include refid="pkwhere" />		
	</select>
	
	<select id="loadDynamic" parameterType="hashmap" resultType="FkDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_fk
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
	</select>

	<select id="count"  parameterType="hashmap" resultType="integer">
		SELECT count(1) 
		FROM t_dic_fk
		<include refid="where" />
	</select>
	
	<select id="countByPK"  resultType="integer">
		SELECT count(1) 
		FROM t_dic_fk
		<include refid="pkwhere" />		
	</select>
	
	<insert id="insert"  parameterType="FkDomain">
		INSERT INTO t_dic_fk (			
				object_id,				
				object_code,				
				object_name,				
				object_comment,				
				parent_table_object_id,				
				child_table_object_id				
		)		
		VALUES (	
			#{objectId},
			#{objectCode},
			#{objectName},
			#{objectComment},
			#{parentTableObjectId},
			#{childTableObjectId}
		) 
	</insert>
	
	<delete id="delete" >
		DELETE FROM t_dic_fk
		<include refid="pkwhere" />	
	</delete>
	
	<delete id="deleteDynamic" parameterType="hashmap">
		DELETE FROM t_dic_fk
		<include refid="where" />
	</delete>

	<update id="update" parameterType="FkDomain">
		UPDATE t_dic_fk	
		<include refid="update" />
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateWithoutNull" parameterType="FkDomain">
		UPDATE t_dic_fk
		<include refid="updateWithoutNull"/>
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateDynamic" parameterType="hashmap">
		UPDATE t_dic_fk	
		<include refid="update" />
		<include refid="where_old" />
	</update>
	
	<select id="loadDynamicPaging" parameterType="hashmap" resultType="FkDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_fk
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
		<if test="_offset != null">LIMIT #{_offset},#{_limit}</if>	
	</select>
</mapper>