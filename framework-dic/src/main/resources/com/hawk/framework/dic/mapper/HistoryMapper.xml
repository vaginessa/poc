<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hawk.framework.dic.mapper.HistoryMapper">

	<sql id="columns">	
				table_name AS "tableName" ,
		version AS "version" ,
		object_id AS "objectId" ,
		object_content AS "objectContent" ,
		create_date AS "createDate" 
	</sql>
	
	<sql id="where">
		<trim prefix="WHERE" prefixOverrides="AND|OR">		 
			<if test="tableName != null"> AND table_name = #{tableName}  </if>
			<if test="version != null"> AND version = #{version}  </if>
			<if test="objectId != null"> AND object_id = #{objectId}  </if>
			<if test="objectContent != null"> AND object_content = #{objectContent}  </if>
			<if test="createDate != null"> AND create_date = #{createDate}  </if>
		</trim>	
	</sql>
	
	<sql id="where_old">
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			AND table_name = #{old_tableName}
			AND version = #{old_version}
			AND object_id = #{old_objectId}
			AND object_content = #{old_objectContent}
			AND create_date = #{old_createDate}
		</trim>
	</sql>
	
	<sql id="update">
		<set>
			table_name = #{tableName},
			version = #{version},
			object_id = #{objectId},
			object_content = #{objectContent},
			create_date = #{createDate},
		</set>
	</sql>
	
	<sql id="updateWithoutNull">
		<set>
			<if test="tableName != null">table_name = #{tableName},</if>
			<if test="version != null">version = #{version},</if>
			<if test="objectId != null">object_id = #{objectId},</if>
			<if test="objectContent != null">object_content = #{objectContent},</if>
			<if test="createDate != null">create_date = #{createDate},</if>
		</set>
	</sql>
	
	<sql id="pkwhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			AND table_name = #{tableName}
			AND version = #{version}
			AND object_id = #{objectId}
		</trim>
	</sql>
	
	<select id="load"  resultType="HistoryDomain">
		SELECT 
		<include refid="columns" />
		FROM t_dic_history
		<include refid="pkwhere" />		
	</select>
	
	<select id="loadDynamic" parameterType="hashmap" resultType="HistoryDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_history
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
	</select>

	<select id="count"  parameterType="hashmap" resultType="integer">
		SELECT count(1) 
		FROM t_dic_history
		<include refid="where" />
	</select>
	
	<select id="countByPK"  resultType="integer">
		SELECT count(1) 
		FROM t_dic_history
		<include refid="pkwhere" />		
	</select>
	
	<insert id="insert"  parameterType="HistoryDomain">
		INSERT INTO t_dic_history (			
				table_name,				
				version,				
				object_id,				
				object_content,				
				create_date				
		)		
		VALUES (	
			#{tableName},
			#{version},
			#{objectId},
			#{objectContent},
			#{createDate}
		) 
	</insert>
	
	<delete id="delete" >
		DELETE FROM t_dic_history
		<include refid="pkwhere" />	
	</delete>
	
	<delete id="deleteDynamic" parameterType="hashmap">
		DELETE FROM t_dic_history
		<include refid="where" />
	</delete>

	<update id="update" parameterType="HistoryDomain">
		UPDATE t_dic_history	
		<include refid="update" />
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateWithoutNull" parameterType="HistoryDomain">
		UPDATE t_dic_history
		<include refid="updateWithoutNull"/>
		<include refid="pkwhere" />	
	</update>
	
	<update id="updateDynamic" parameterType="hashmap">
		UPDATE t_dic_history	
		<include refid="update" />
		<include refid="where_old" />
	</update>
	
	<select id="loadDynamicPaging" parameterType="hashmap" resultType="HistoryDomain">
		SELECT 		
		<include refid="columns" />
		FROM t_dic_history
		<include refid="where" />
		<if test="_orderby != null">ORDER BY ${_orderby}</if>
		<if test="_offset != null">LIMIT #{_offset},#{_limit}</if>	
	</select>
</mapper>